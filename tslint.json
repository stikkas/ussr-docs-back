{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [
            false
        ],
        "no-empty": [
            true,
            "allow-empty-catch",
            "allow-empty-functions"
        ],
        "quotemark": [
            true,
            "single"
        ],
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}