        "@types/body-parser": "^1.17.0",
        "@types/express": "^4.16.1",
        "@types/mkdirp": "^0.5.2",
        "@types/node": "^11.11.3",
        "nodemon": "^1.18.10",
        "ts-node": "^8.0.3",
        "tslint": "^5.14.0",
        "typescript": "^3.3.3333"
    }
}
